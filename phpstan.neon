parameters:
  level: max
  paths:
    - src
  excludePaths:
    - src/DataFixtures
    - src/Command
    #- src/Services/Dashboard/DashboardService.php
  checkMissingIterableValueType: true
  reportUnmatchedIgnoredErrors: false
  editorUrl: 'phpstorm://open?file=%%file%%&line=%%line%%'
  editorUrlTitle: '%%relFile%%:%%line%%'
